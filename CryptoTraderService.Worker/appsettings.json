{
  "ServiceConfigurations": {
    "Host": "https://api.bitcointrade.com.br",
    "Token": "",
    "Interval": 180000, //3 min
    "LimitAmount": 100,
    "MinValue": 681,
    "MaxValue": 710,
    "Subtype": "limited"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\CryptoTrader\\File.log",
          "fileSizeLimitBytes": "10000",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "20",
          "rollOnFileSizeLimit": "true",
          "shared": "true"
        }
      }
    ]
  }
}